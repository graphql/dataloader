  "files": [
    "index.js",
    "index.d.ts",
    "README.md",
    "LICENSE",
    "PATENTS"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@types/jest": "27.4.0",
    "eslint": "6.6.0",
    "jest": "24.9.0",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5",
    "sane": "4.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
