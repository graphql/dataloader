  },
  "repository": {
    "type": "git",
    "url": "http://github.com/graphql/dataloader.git"
  },
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "npm run lint && npm run check && npm run testonly",
    "test:ci": "npm run lint && npm run check && npm run testonly -- --coverage",
